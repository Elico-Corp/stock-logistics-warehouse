<?xml version="1.0"?>
<openerp>
  <data>
    <record model="ir.ui.view" id="stock_picking_out_form">
      <field name="name">stock.picking.out.form.date_pickup</field>
      <field name="model">stock.picking.out</field>
      <field name="inherit_id" ref="stock.view_picking_out_form" />
      <field name="arch" type="xml">
        <xpath expr="//field[@name='min_date']" position="after">
          <field name="date_pickup" />
        </xpath>
      </field>
    </record>
  </data>
</openerp>

